{
    "version": "0.2.4",
    "configurations": [
        {
            "name": "WAPT: install",
            "type": "python",
            "request": "launch",
            "justMyCode": false,
            "program": "${config:python.wapt-get}",
            "args": [
                "install",
                "--no-ide",
                "${workspaceFolder}"
            ],
            "console": "integratedTerminal",
            "linux": {
                "sudo": true
            },
            "osx": {
                "sudo": true
            },
            "python": "${command:python.interpreterPath}",
            "pythonArgs": [
                "-I"
            ]
        },
